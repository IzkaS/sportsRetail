   "source": [
    "%%sql\n",
    "\n",
    "-- Selecting brand and average_discount as a percentage\n",
    "\n",
    "SELECT \n",
    "        brand,\n",
    "        AVG(discount)*100 AS average_discount\n",
    "FROM brands AS b\n",
    "JOIN finance AS f\n",
    "ON b.product_id = f.product_id\n",
    "WHERE brand IS NOT NULL\n",
    "GROUP BY brand;"
