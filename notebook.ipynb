    "<tr>\n",
    "<td><code>product_name</code></td>\n",
    "<td><code>varchar</code></td>\n",
    "<td>Name of the product</td>\n",
    "</tr>\n",
    "<tr>\n",
    "<td><code>product_id</code></td>\n",
    "<td><code>varchar</code></td>\n",
    "<td>Unique ID for product</td>\n",
    "</tr>\n",
    "<tr>\n",
