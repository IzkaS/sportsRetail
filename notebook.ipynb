    "%%sql\n",
    "\n",
    "-- Calculating the correlation between reviews and revenue as review_revenue_corr\n",
    "\n",
    "SELECT\n",
    "        CORR(reviews, revenue) AS review_revenue_corr\n",
    "FROM reviews AS r\n",
    "JOIN finance AS f\n",
    "ON r.product_id = f.product_id;"
