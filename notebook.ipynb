   "source": [
    "%%sql\n",
    "\n",
    "-- Filtering for products with a listing_price more than zero\n",
    "\n",
    "SELECT \n",
    "        brand,\n",
    "        listing_price::INTEGER,\n",
    "        COUNT(f.product_id)\n",
    "        \n",
    "FROM finance AS f\n",
    "JOIN brands AS b\n",
    "ON f.product_id = b.product_id\n",
    "WHERE listing_price::INTEGER > 0\n",
    "GROUP BY brand, listing_price\n",
    "ORDER BY listing_price::INTEGER DESC;\n",
    "        "
   ]
  },
